<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/fl_util_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">


    <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/homeHeadColor"
            android:layout_height="@dimen/keyboard_top_view_tip_container_height"
            android:layout_gravity="bottom"
            android:gravity="center"
            android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
            android:id="@+id/keyboard_top_view_tip_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:minHeight="@dimen/keyboard_top_view_tip_container_height"
            android:gravity="bottom"
            android:layout_gravity="bottom"
            android:orientation="horizontal">


        <com.tree.newidea.view.AutomaticExpansionLinearLayout
                android:id="@+id/keyboard_top_view_first_txt"
                android:background="@drawable/app_shape_utile_bar_item"
                android:animateLayoutChanges="true"
                android:layout_weight="1"
                android:gravity="center_horizontal|bottom"
                android:minHeight="@dimen/keyboard_top_view_tip_container_height"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_first_txt6"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="###### "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_first_txt5"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="##### "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_first_txt4"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="#### "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_first_txt3"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="### "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_first_txt2"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="## "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_first_txt1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="# "
                    android:textSize="14sp"/>


        </com.tree.newidea.view.AutomaticExpansionLinearLayout>


        <com.tree.newidea.view.AutomaticExpansionLinearLayout
                android:id="@+id/keyboard_top_view_second_txt"
                android:background="@drawable/app_shape_utile_bar_item"
                android:animateLayoutChanges="true"
                android:layout_weight="1"
                android:gravity="center_horizontal|bottom"
                android:minHeight="@dimen/keyboard_top_view_tip_container_height"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_second_txt6"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text=">>>>>> "
                    android:textSize="14sp"/>

            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_second_txt5"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text=">>>>> "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_second_txt4"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text=">>>> "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_second_txt3"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text=">>> "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_second_txt2"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text=">> "
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_second_txt1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="> "
                    android:textSize="14sp"/>

        </com.tree.newidea.view.AutomaticExpansionLinearLayout>


        <com.tree.newidea.view.AutomaticExpansionLinearLayout

                android:id="@+id/keyboard_top_view_third_txt"
                android:background="@drawable/app_shape_utile_bar_item"
                android:animateLayoutChanges="true"
                android:layout_weight="1"
                android:gravity="center_horizontal|bottom"
                android:minHeight="@dimen/keyboard_top_view_tip_container_height"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content">


            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_third_txt4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="~~~~"
                    android:textSize="14sp"/>

            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_third_txt3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="******"
                    android:textSize="14sp"/>

            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_third_txt2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="**"
                    android:textSize="14sp"/>

            <TextView
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:id="@+id/keyboard_top_view_third_txt1"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="****"
                    android:textSize="14sp"/>


            <TextView
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:id="@+id/keyboard_top_view_third_txt6"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:maxLines="1"
                    android:text="------------------------------"
                    android:textSize="14sp"/>


            <TextView
                    android:layout_width="match_parent"
                    android:id="@+id/keyboard_top_view_third_txt5"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="* "
                    android:textSize="14sp"/>

        </com.tree.newidea.view.AutomaticExpansionLinearLayout>


        <com.tree.newidea.view.AutomaticExpansionLinearLayout
                android:id="@+id/keyboard_top_view_fourth_txt"
                android:background="@drawable/app_shape_utile_bar_item"
                android:animateLayoutChanges="true"
                android:layout_weight="1"
                android:gravity="center_horizontal|bottom"
                android:minHeight="@dimen/keyboard_top_view_tip_container_height"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_fourth_txt4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text='![]("")'
                    android:textSize="14sp"/>

            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_fourth_txt3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="![]()"
                    android:textSize="14sp"/>


            <TextView
                    android:visibility="gone"
                    android:id="@+id/keyboard_top_view_fourth_txt2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text='[]("")'
                    android:textSize="14sp"/>

            <TextView
                    android:id="@+id/keyboard_top_view_fourth_txt1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/keyboard_top_view_tip_container_height"
                    android:gravity="center"
                    android:text="[]()"
                    android:textSize="14sp"/>

        </com.tree.newidea.view.AutomaticExpansionLinearLayout>

    </LinearLayout>


</FrameLayout>



